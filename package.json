    "vue": "^2.4.4",
    "vue-swal": "0.0.6",
    "vuex": "^2.4.1",
    "vuex-i18n": "^1.7.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "electron": "^1.7.9",
    "electron-builder": "^19.37.2",
    "file-loader": "^1.1.4",
    "node-sass": "^4.5.3",
